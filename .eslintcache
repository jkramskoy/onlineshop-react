r() {\n    return (\n      <div>\n        Product Name :<label>{this.props.name}</label>\n        <br />\n        CAD{this.props.price}\n        <br />\n        <img src={this.props.photoPath} width=\"200\" />\n        <br />\n        <input\n          type=\"number\"\n          min=\"1\"\n          max=\"20\"\n          onChange={(event) => {\n            this.setState({ qty: event.target.value });\n          }}\n          disabled={this.state.selected}\n        />\n        {this.state.selected ? <span> added to Card</span> : null}\n        <br />\n        <button\n          onClick={() => {\n            this.addToCartClicked(this.props.id, this.state.qty);\n          }}\n          disabled={this.state.selected}\n        >\n          Add to Cart\n        </button>\n        <br />\n        Description:\n        <label>{this.props.description}</label>\n        <hr />\n      </div>\n    );\n  }\n}\n\nexport default Product;\n","/Users/juliakramskoy/VSCodeProjects/onlineshop-react/src/components/Products/CreateProduct.js",[],"/Users/juliakramskoy/VSCodeProjects/onlineshop-react/src/containers/Products/Products.js",["53"],"import React, { Component } from \"react\";\nimport Product from \"../../components/Products/Product\";\nimport axios from \"axios\";\nimport CreateProduct from \"../../components/Products/CreateProduct\";\nimport { Header, Segment } from \"semantic-ui-react\";\nimport { Button } from \"semantic-ui-react\";\n\nclass Products extends Component {\n  //connect with API\n  state = {\n    products: [],\n    url: \"https://localhost:5001/Product/\",\n    cartItems: [],\n  };\n\n  //take data from database and store\n  componentDidMount() {\n    //console.log(\"componentDidMount\");\n    axios.get(this.state.url).then((response) => {\n      //console.log(response);\n      this.setState({ products: response.data });\n    });\n  }\n\n  //test request to API go through all objects and get by id product\n  getProductByIdHandler = (id) => {\n    axios.get(this.state.url + id).then((response) => {\n      console.log(response);\n    });\n  };\n\n  addToCartHandler = (id, q) => {\n    // console.log(\" parent:  \" + id + \" \" + q);\n\n    let ci = this.state.cartItems;\n    ci.push({ id: id, quantity: +q });\n\n    this.setState({ cartItems: ci });\n\n    console.log(this.state.cartItems);\n  };\n\n  orderNowHandler = () => {\n    const queryParams = [];\n\n    for (let i of this.state.cartItems) {\n      if (i !== null) {\n        queryParams.push(i.id + \"=\" + i.quantity);\n      }\n    }\n    const queryStr = queryParams.join(\"&\");\n\n    this.props.history.push({\n      pathname: \"/cart\",\n      search: \"?\" + queryStr,\n    });\n  };\n\n  render() {\n    //convert data from JASON file\n    const productList = this.state.products.map((pr) => {\n      return (\n        <Product\n          key={pr.id}\n          id={pr.id}\n          name={pr.name}\n          price={pr.price}\n          photoPath={pr.photoPath}\n          description={pr.description}\n          getByIdClicked={() => this.getProductByIdHandler(pr.id)}\n          addToCartClicked={this.addToCartHandler}\n        />\n      );\n    });\n\n    return (\n      <div>\n        <Segment clearing>\n          <Header\n            as=\"h2\"\n            dividing\n            floated=\"left\"\n            color=\"black\"\n            background-color=\"teal\"\n          >\n            Please select the products you want â€¦.\n          </Header>\n        </Segment>\n        {productList}\n        {/* <CreateProduct /> */}\n\n        <div\n          style={{\n            textAlign: \"right\",\n            top: \"80px\",\n            right: \"60px\",\n            position: \"fixed\",\n          }}\n        >\n          <Button onClick={this.orderNowHandler} color=\"teal\">\n            ORDER NOW!\n          </Button>\n        </div>\n      </div>\n    );\n  }\n}\n\nexport default Products;\n","/Users/juliakramskoy/VSCodeProjects/onlineshop-react/src/containers/Login